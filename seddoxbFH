<script>

function handleSubmit(event) {
    event.preventDefault();
   
    
    const data = new FormData(event.target);
   const value = Object.fromEntries(data.entries());
   const elements = event.target.elements;
    console.log("1")
    console.log(value);
    
    var obj ={};
    for(var i = 0 ; i < elements.length ; i++){
        var item = elements.item(i);
        obj[item.name] = item.value;
    }
    
    console.log(obj)


fetch("https://hook.us1.make.com/7hghfdoq8a3ccq8mvtoolv72vq7fkqer",
    {
        body: JSON.stringify(value),
        headers: {
            Accept: 'application/json',
            'Content-Type': 'application/json',
        },
        method: "post",
       
    }) 
    .then((value) =>{
        console.log('todo ok')
        //evento();
        //alert("Formulario enviado existosamente");
        Swal.fire({
        title: "Mensaje enviado, muy pronto te contactaremos",
        icon: "success",
        confirmButtonColor: "#000000",
        confirmButtonText: "Cerrar",
      });
      document.getElementById("form-field-especialidad").value = "";
    document.getElementById("form-field-name").value = "";
    
    document.getElementById("form-field-last_name").value = "";
    document.getElementById("form-field-email").value = "";
    document.getElementById("form-field-phone").value = "";
    console.log('barredora ok');
    })
    .catch((error) => {
    console.error('Error:', error);
  });
  }
 
 function evento() {
    //Se cambia el nombre del evento que se necesita que se dispare en google y facebook
    var evento= "formulario-enviado-landing";
    fbq("trackCustom", evento), 
    gtag("event", evento, 
    { event_category: "Form", 
    event_label: evento });
}
 
 
    function addEvent() {
        // Se coloca ID del formulario que se configura en additional options en la edición del formulario de elementor
        // si existen más de un formulario en la web se debe cambiar la variable form agregando un numero al final
        const form = document.querySelector('#form_landing');
        console.log("form",form);
        if(!form){"No se encontro formulario con id"};
      form.addEventListener('submit', handleSubmit);
    }
  
  window.onload= addEvent();
 


</script>				</div>		</div>
